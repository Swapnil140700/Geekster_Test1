
import java.util.Scanner;

public class Solution {
    public static void main(String args[])
    {
        int num,exp,power=1;
        Scanner sc=new Scanner(System.in);
        num=sc.nextInt();
        exp=sc.nextInt();
        for (int i=1;i<=exp;i++)
        {
            power=power*num;
        }
        System.out.println(power);

    }
}
