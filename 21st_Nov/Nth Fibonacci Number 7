import java.io.*;
import java.util.*;

public class Solution {
    public static void main(String  args[])
    {
        Scanner sc=new Scanner(System.in);
        int count= sc.nextInt();
        int num1=1,num2=1,num3;
        for (int i=2;i<count;i++)
        {
            num3=num1+num2;
            num1=num2;
            num2=num3;

        }
        System.out.println(num2);
    }
}

