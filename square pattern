import java.util.Scanner;

public class pattern {
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        
        int num=sc.nextInt();
        for (int i=num;i>0;i--)
        {
           
            for (int j=2;j<=num;j++)
            {
                System.out.print(num);
            }
             System.out.println(num);
        }
    }
}
